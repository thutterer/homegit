"*":
  "autocomplete-plus":
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  "autocomplete-ruby":
    rsensePath: "/home/tom/.rvm/gems/ruby-2.4.1@atom/bin/rsense"
  autosave:
    enabled: true
  core:
    autoHideMenuBar: true
    closeDeletedFileTabs: true
    disabledPackages: [
      "autocomplete-ruby"
      "symbols-view"
      "metrics"
      "welcome"
    ]
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "no"
  editor: {}
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".erb.html.text":
  editor:
    showIndentGuide: true
    showInvisibles: false
".ruby.source":
  editor:
    showIndentGuide: true
